<?xml version="1.0" encoding="UTF-8"?>
<linkbase xmlns="http://www.xbrl.org/2003/linkbase"
          xmlns:xlink="http://www.w3.org/1999/xlink"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    
    <calculationLink xlink:type="extended" xlink:role="http://fasb.org/us-gaap/role/statement/StatementOfIncome">
        
        <!-- Locators -->
        <loc xlink:href="../elts/us-gaap-2020.xsd#us-gaap_OperatingCostsAndExpenses" 
             xlink:label="loc_OperatingCostsAndExpenses" xlink:type="locator"/>
        
        <loc xlink:href="../elts/us-gaap-2020.xsd#us-gaap_ResearchAndDevelopmentExpense" 
             xlink:label="loc_ResearchAndDevelopmentExpense" xlink:type="locator"/>
        
        <loc xlink:href="../elts/us-gaap-2020.xsd#us-gaap_ResearchAndDevelopmentExpenseExcludingAcquiredInProcessCost" 
             xlink:label="loc_ResearchAndDevelopmentExpenseExcludingAcquiredInProcessCost" xlink:type="locator"/>
        
        <loc xlink:href="../elts/us-gaap-2020.xsd#us-gaap_ResearchAndDevelopmentExpenseSoftwareExcludingAcquiredInProcessCost" 
             xlink:label="loc_ResearchAndDevelopmentExpenseSoftwareExcludingAcquiredInProcessCost" xlink:type="locator"/>
        
        <loc xlink:href="../elts/us-gaap-2020.xsd#us-gaap_ResearchAndDevelopmentInProcess" 
             xlink:label="loc_ResearchAndDevelopmentInProcess" xlink:type="locator"/>
        
        <loc xlink:href="../elts/us-gaap-2020.xsd#us-gaap_SellingGeneralAndAdministrativeExpense" 
             xlink:label="loc_SellingGeneralAndAdministrativeExpense" xlink:type="locator"/>
        
        <loc xlink:href="../elts/us-gaap-2020.xsd#us-gaap_DepreciationAndAmortization" 
             xlink:label="loc_DepreciationAndAmortization" xlink:type="locator"/>
        
        <loc xlink:href="../elts/us-gaap-2020.xsd#us-gaap_NetIncomeLoss" 
             xlink:label="loc_NetIncomeLoss" xlink:type="locator"/>
        
        <loc xlink:href="../elts/us-gaap-2020.xsd#us-gaap_Revenues" 
             xlink:label="loc_Revenues" xlink:type="locator"/>
        
        <!-- Calculation Arcs -->
        <!-- OperatingCostsAndExpenses = R&D + SG&A + D&A -->
        <calculationArc xlink:type="arc" 
                        xlink:arcrole="http://www.xbrl.org/2003/arcrole/summation-item"
                        xlink:from="loc_OperatingCostsAndExpenses" 
                        xlink:to="loc_ResearchAndDevelopmentExpense"
                        order="10" weight="1.0"/>
        
        <calculationArc xlink:type="arc" 
                        xlink:arcrole="http://www.xbrl.org/2003/arcrole/summation-item"
                        xlink:from="loc_OperatingCostsAndExpenses" 
                        xlink:to="loc_SellingGeneralAndAdministrativeExpense"
                        order="20" weight="1.0"/>
        
        <calculationArc xlink:type="arc" 
                        xlink:arcrole="http://www.xbrl.org/2003/arcrole/summation-item"
                        xlink:from="loc_OperatingCostsAndExpenses" 
                        xlink:to="loc_DepreciationAndAmortization"
                        order="30" weight="1.0"/>
        
        <!-- R&D = R&D_ExcludingAcquired + R&D_Software + R&D_InProcess -->
        <calculationArc xlink:type="arc" 
                        xlink:arcrole="http://www.xbrl.org/2003/arcrole/summation-item"
                        xlink:from="loc_ResearchAndDevelopmentExpense" 
                        xlink:to="loc_ResearchAndDevelopmentExpenseExcludingAcquiredInProcessCost"
                        order="10" weight="1.0"/>
        
        <calculationArc xlink:type="arc" 
                        xlink:arcrole="http://www.xbrl.org/2003/arcrole/summation-item"
                        xlink:from="loc_ResearchAndDevelopmentExpense" 
                        xlink:to="loc_ResearchAndDevelopmentExpenseSoftwareExcludingAcquiredInProcessCost"
                        order="20" weight="1.0"/>
        
        <calculationArc xlink:type="arc" 
                        xlink:arcrole="http://www.xbrl.org/2003/arcrole/summation-item"
                        xlink:from="loc_ResearchAndDevelopmentExpense" 
                        xlink:to="loc_ResearchAndDevelopmentInProcess"
                        order="30" weight="1.0"/>
        
        <!-- NetIncomeLoss = Revenues - OperatingCostsAndExpenses (simplified) -->
        <calculationArc xlink:type="arc" 
                        xlink:arcrole="http://www.xbrl.org/2003/arcrole/summation-item"
                        xlink:from="loc_NetIncomeLoss" 
                        xlink:to="loc_Revenues"
                        order="10" weight="1.0"/>
        
        <calculationArc xlink:type="arc" 
                        xlink:arcrole="http://www.xbrl.org/2003/arcrole/summation-item"
                        xlink:from="loc_NetIncomeLoss" 
                        xlink:to="loc_OperatingCostsAndExpenses"
                        order="20" weight="-1.0"/>
        
    </calculationLink>
</linkbase>